.container {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  width: auto;
  height: auto;
  margin: 50rpx;
  border: 2rpx solid #f0f0f0;
  border-radius: 15rpx;
  padding: 10rpx;
}

.content {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex: 1;
}

.content van-icon {
  margin: 24rpx;
}
